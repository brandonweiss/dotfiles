"*":
  "atom-beautify":
    _analyticsUserId: "0ed61702-263b-4138-961b-6190a4b74f9f"
  "atom-ide-ui":
    hyperclick:
      darwinTriggerKeys: "altKey,metaKey"
    use: {}
  "atom-package-deps":
    ignored: []
  "atom-runner": {}
  "autocomplete-plus":
    confirmCompletion: "tab always, enter when suggestion explicitly selected"
  "bracket-matcher":
    highlightMatchingLineNumber: true
  core:
    disabledPackages: [
      "language-php"
      "language-java"
      "language-c"
      "language-objective-c"
      "welcome"
      "language-clojure"
      "language-csharp"
      "language-make"
      "test-status"
      "atom-html-preview"
      "emmet"
      "ember-cli-helper"
      "git-time-machine"
      "atom-runner"
      "regex-railroad-diagram"
      "atom-ide-ui"
      "language-rust"
      "rails-transporter"
      "linter-stylelint"
      "language-lua"
    ]
    titleBar: "custom-inset"
    useTreeSitterParsers: true
  editor:
    autoIndentOnPaste: false
    fontFamily: "Operator Mono"
    fontSize: 17
    invisibles: {}
    preferredLineLength: 100
    showIndentGuide: true
    softWrapAtPreferredLineLength: true
  "exception-reporting":
    userId: "d860e2ce-b841-f643-3414-b61df9ea2275"
  "find-and-replace": {}
  "fuzzy-finder":
    preserveLastSearch: true
    scoringSystem: "fast"
    useRipGrep: true
  "linter-jshint":
    disableWhenNoJshintrcFileInPath: true
  "linter-rubocop":
    command: "/Users/brandon/.rbenv/versions/2.4.1/bin/rubocop"
  "linter-ui-default":
    panelHeight: 69
    showPanel: true
  metrics:
    userId: "78d13bac588a8fc8b6c7e7e5427cd45c06007882"
  minimap: {}
  react: {}
  runner:
    scopes:
      ruby: "/usr/local/bin/rbenv exec ruby"
  "split-diff":
    diffWords: true
    ignoreWhitespace: true
    leftEditorColor: "red"
    rightEditorColor: "green"
    syncHorizontalScroll: true
  "status-bar":
    fullWidth: false
  tabs:
    enableVcsColoring: true
  "test-status": {}
  "tree-view":
    hideIgnoredNames: true
    hideVcsIgnoredFiles: true
  welcome:
    showOnStartup: false
".mdx.source":
  editor:
    softWrap: true
".plain.text":
  editor:
    softWrap: true
".python.source":
  editor:
    tabLength: 2
