{
  "git.path" : "\/usr\/local\/bin\/git",
  "[mdx]" : {
    "editor.wordWrap" : "wordWrapColumn"
  },
  "editor.scrollBeyondLastLine" : false,
  "editor.fontFamily" : "Operator Mono",
  "workbench.colorTheme" : "One Dark",
  "workbench.iconTheme" : "city-lights-icons-vsc",
  "search.useGlobalIgnoreFiles" : true,
  "editor.tabSize" : 2,
  "editor.find.addExtraSpaceOnTop" : false,
  "files.trimTrailingWhitespace" : true,
  "editor.fontSize" : 17,
  "eslint.validate" : [
    "mdx"
  ],
  "editor.tokenColorCustomizations" : {
    "textMateRules": [
      {
        "name": "One Dark italic",
        "scope": [
          "comment",
          "entity.other.attribute-name",
          "keyword",
          "markup.underline.link",
          "storage.modifier",
          "storage.type",
          "string.url",
          "variable.language.super",
          "variable.language.this"
        ],
        "settings": {
          "fontStyle": "italic"
        }
      },
      {
        "name": "One Dark italic reset",
        "scope": [
          "keyword.operator",
          "keyword.other.type",
          "storage.modifier.import",
          "storage.modifier.package",
          "storage.type.built-in",
          "storage.type.function.arrow",
          "storage.type.generic",
          "storage.type.java",
          "storage.type.primitive"
        ],
        "settings": {
          "fontStyle": ""
        }
      }
    ]
  },
  "eslint.probe" : [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "html",
    "vue",
    "markdown",
    "mdx"
  ],
  "workbench.colorCustomizations" : {
    "terminal.ansiBrightCyan" : "#76D4D6",
    "terminal.ansiGreen" : "#A8D4A9",
    "terminal.ansiBrightYellow" : "#FFD479",
    "terminal.ansiBrightRed" : "#F78D8C",
    "terminal.ansiYellow" : "#FFD479",
    "terminal.ansiBlue" : "#69ACDA",
    "terminal.ansiMagenta" : "#D7ACD6",
    "terminal.ansiCyan" : "#76D4D6",
    "terminal.ansiBrightWhite" : "#D6D6D6",
    "terminal.ansiBlack" : "#646464",
    "terminal.ansiBrightMagenta" : "#D7ACD6",
    "terminal.ansiWhite" : "#D6D6D6",
    "terminal.ansiBrightBlue" : "#69ACDA",
    "terminal.ansiRed" : "#F78D8C",
    "terminal.ansiBrightBlack" : "#646464",
    "terminal.ansiBrightGreen" : "#A8D4A9"
  },
  "editor.wordWrapColumn" : 100,
  // "terminal.integrated.shell.osx" : "\/bin\/bash",
  "terminal.integrated.shell.osx" : "\/opt\/homebrew\/bin\/fish",
  "files.insertFinalNewline" : true,
  "[fish]" : {
    "editor.defaultFormatter" : "bmalehorn.vscode-fish"
  },
  "[markdown]" : {
    "editor.wordWrap" : "wordWrapColumn"
  },
  "workbench.startupEditor" : "newUntitledFile",
  "editor.formatOnPaste" : true,
  "editor.formatOnSave" : true,
  "[json]" : {
    "editor.defaultFormatter" : "esbenp.prettier-vscode"
  },
  "search.exclude" : {
    "package-lock.json" : true
  },
  "[javascriptreact]" : {
    "editor.defaultFormatter" : "esbenp.prettier-vscode"
  },
  "editor.rulers" : [
    100
  ],
  "problems.showCurrentInStatus" : true,
  "editor.accessibilitySupport" : "off",
  "editor.formatOnType" : true,
  "[html]" : {
    "editor.defaultFormatter" : "esbenp.prettier-vscode"
  },
  "terminal.integrated.automationShell.linux": "",
  "terminal.integrated.env.osx": {
    "FIG_NEW_SESSION": "1"
  }
}
